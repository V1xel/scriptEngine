        },
        "Concat": {
            "description": "Convert object to number",
            "type": "object",
            "allOf": [
                {
                    "$ref": "literalExpression.json#/Primitive"
                }
            ],
            "properties": {
                "Separator": {
                    "enum": ["."]
                } 
            }
        },
        "ToLiteral": {
            "description": "Convert object to number",
            "type": "object",
            "oneOf": [
                {
                    "$ref": "expression.json#"
                }
            ]
        }
    },
    "Primitive": {
        "properties": {
            "left": {
                "oneOf": [
                    {
                        "$ref": "expression.json#"
                    }
                ]
            },
            "right": {
                "oneOf": [
                    {
                        "$ref": "expression.json#"
                    }
                ]
            }
        }
    }
}