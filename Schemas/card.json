{
    "$schema": "./expressions.json",
    "Expression": {
        "Branch": {
            "Condition": {
                "GreaterThan": {
                    "left": {
                        "Number": 5
                    },
                    "right": {
                        "ToNumber": {
                            "Expression": {
                                "Literal": "25"
                            }
                        }
                    }
                }
            },
            "OnTrue": {
                "Expression": {
                    "Announce": {
                        "EventName": "Comes to the Arena",
                        "CallerId": 1
                    }
                }
            }
        }
    }
}