{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "properties": {
        "ToBool": {
            "type": "object",
            "oneOf": [
                {
                    "$ref": "expression.json#"
                }
            ]
        } ,
        "GreaterThan": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "logicalExpression.json#/NumericPrimitive"
                }
            ]
        },
        "LowerThan": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "logicalExpression.json#/NumericPrimitive"
                }
            ]
        },
        "Equal": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "logicalExpression.json#/NumericPrimitive"
                }
            ]
        },
        "NotEqual": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "logicalExpression.json#/NumericPrimitive"
                }
            ]
        },
        "And": {
            "properties": {
                "left": {
                    "oneOf": [
                        {
                            "$ref": "logicalExpression.json#"
                        }
                    ]
                },
                "right": {
                    "oneOf": [
                        {
                            "$ref": "logicalExpression.json#"
                        }
                    ]
                }
            }
        },
        "Or": {
            "properties": {
                "left": {
                    "oneOf": [
                        {
                            "$ref": "logicalExpression.json#"
                        }
                    ]
                },
                "right": {
                    "oneOf": [
                        {
                            "$ref": "logicalExpression.json#"
                        }
                    ]
                }
            }
        }
    },
    "NumericPrimitive": {
        "properties": {
            "left": {
                "oneOf": [
                    {
                        "$ref": "expression.json#"
                    }
                ]
            },
            "right": {
                "oneOf": [
                    {
                        "$ref": "expression.json#"
                    }
                ]
            }
        }
    }
}